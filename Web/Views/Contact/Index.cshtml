@model Common.ContactPageDto 
@{
    ViewData["Title"] = "New List Page";
}
@section Styles {
  <link href="/css/contact.css" rel="stylesheet" />
}
<h2>两委班子</h2>
<div class="newdetailpart">
  @if (Model.contactList != null)
  {
    foreach (var contact in Model.contactList)
    {
      <div class="card">
          <img src="@contact.personHead" width="100" height="100" alt="头像">
          <div class="card-content">
              <h3>@contact.personName</h3>
              <p>@contact.post</p>
              <p>@contact.depent</p>
          </div>
          <div class="card-actions">
              <button onclick="window.location.href='/Contact/Resume?id=@contact.id'">简历</button>
              <button onclick="window.location.href='/Contact/Write?id=@contact.id'">给我写信</button>
          </div>
      </div>
    }
  }
</div>
@section Scripts {
    <script>
        
    </script>
}